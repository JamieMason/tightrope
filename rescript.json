{
  "name": "tightrope",
  "bs-dependencies": ["@rescript/core"],
  "bsc-flags": ["-open RescriptCore"],
  "suffix": ".bs.js",
  "uncurried": true,
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "gentypeconfig": {
    "language": "typescript",
    "module": "es6",
    "importPath": "relative",
    "generatedFileExtension": ".gen.ts",
    "shims": {},
    "debug": {
      "all": false
    },
    "exportInterfaces": true
  },
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ]
}
